generator client {
  provider   = "prisma-client"
  output     = "../lib/generated/prisma"
  engineType = "client" // enable Prisma ORM without Rust
}

generator json {
  provider  = "prisma-json-types-generator"
  namespace = "PrismaJson"
  allowAny  = false
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
